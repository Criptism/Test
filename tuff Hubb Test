local Tab = Window:CreateTab("Farming")
local FESection = Tab:CreateSection("Farming & Equip")
local EToggle = Tab:CreateToggle({
	Name = "Equip Toggle",
	CurrentValue = false,
	Flag = "Equip1",
	Callback = function(state)
		print(state)
		if state then
			print("Toggle On")
		_G.Equip = true
		if _G.Equip == true then
			wait()
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v.Name == "Double Weight" then
					v.Parent = game.Players.LocalPlayer.Character
				end
			end
		end
		else
		print("Toggle Off")
		_G.Equip = false
		if _G.Equip == false then 
		for i,v in pairs(player.Character:GetChildren()) do
			if v.Name == "Double Weight" then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
	end
	end,
})

local FToggle = Tab:CreateToggle({
	Name = "Farm Toggle",
	CurrentValue = false,
	Flag = "Farm",
	Callback = function(state)
		print(state)
		if state then 
			while _G.Farm do
				wait(0.55)
				for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
					if v.Name == "Double Weight" then
						v:Activate() 
					end
				end
			end
		end
		else
			_G.Farm = false
		end
	end,
})

local Tab = Window:CreateTab("Dupe")
local DButtons = Tab:CreateButton({
	Name = "Dupe 750 Weights",
	CurrentValue = false,
	Callback = function(state)
		if state then
			_G.running = true
			if str >= 80000 then
				runLoop(750)
			end
		end
	end,
})

local DButtons = Tab:CreateButton({
	Name = "Dupe 70 Weights",
	CurrentValue = false,
	Callback = function(state)
		if state then
			_G.running = true
			if str < 80000 then
				runLoop(70)
			end
		end
	end,
})

function runLoop(limit)
	task.spawn(function()
		local count = 1
		while _G.running and count == limit do
			local function simulatePurchase(gamePassId)
				MarketplaceService:SignalPromptGamePassPurchaseFinished(LocalPlayer, gamePassId, true)
			end

			simulatePurchase(5949054)
			count += 1
			task.wait(0.55)
		end
		_G.running = false
	end)
end
local Label2 = Tab:CreateLabel("Weight: 0")

task.spawn(function()
    repeat
        task.wait(1)
        local num = 0
        local backpackItems = LocalPlayer.Backpack:GetChildren()
        for i, v in backpackItems do
            if v.Name == "Double Weight" then
                num += 1
            end
        end

        if num == 0 then
            local characterItems = character:GetChildren()
            for i, v in characterItems do
                if v.Name == "Double Weight" then
                    num += 1
                end
            end
        end

        Label2:Set("Weight: " .. num)
    until num == 9999999999999
end)

local Tab = Window:CreateTab("Information")
local Label3 = Tab:CreateLabel("Strength: 0")
task.spawn(function()
	while true do
		task.wait(1)
		str:GetPropertyChangedSignal("Value"):Connect(function()
			Label3:Set("Strength: " .. Strength.Value)
		end)
	end
end)
local Label = Tab:CreateLabel("Gained: 0")
task.spawn(function()
	repeat
		task.wait(1)
	local curvalue = str.Value;
	local earned = 0.00;
	str:GetPropertyChangedSignal("Value"):Connect(function()
		local delta = (str.Value - curvalue);
		earned += delta;
		script.Parent.Text = "Earned: "..earned;
		curvalue = str.Value;
	end)

	Label:Set("Gained: " .. earned)
	until earned == 999999999999999999999999999999999999999999
end)

