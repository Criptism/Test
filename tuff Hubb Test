        ["content"] = " **Premium Script Executed**",
        ["embeds"] = {
            {
                ["title"] = "**TuFF Hub** | Executed by: "..playerName,
                ["type"] = "rich",
                ["color"] = tonumber(0xff8000),
                ["fields"] = {
                    {

                        ["name"] = "Username: "..playerName,
